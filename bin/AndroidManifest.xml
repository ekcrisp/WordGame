        </activity>
        <activity
             android:name="edu.bard.wordgame.EditLevelsActivity">
        </activity>
        <activity
             android:name="edu.bard.wordgame.EditSingleLevelActivity">
        </activity>
        <activity
            android:name="edu.bard.wordgame.LevelActivity">
        </activity>
        
    </application>

</manifest>
